{
  "name": "gatsby-plugin-astroturf",
  "description": "Gatsby plugin for styling with astroturf",
  "version": "0.0.1",
  "author": "Matija MarohniÄ‡ <matija.marohnic@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "astroturf"
  ],
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "browserslist": [
    "node 6"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --findRelatedTests"
    ]
  },
  "peerDependencies": {
    "astroturf": "*",
    "gatsby": ">=2"
  },
  "dependencies": {
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  }
}
